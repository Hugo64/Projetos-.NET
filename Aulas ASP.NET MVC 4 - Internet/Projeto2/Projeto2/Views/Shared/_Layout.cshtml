<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <h1>System Hacker</h1>
    @Html.ActionLink("Página inicial", "Index", "Home")
    @Html.ActionLink("Programação C#", "programacaoCsharp", "Home")
    @Html.ActionLink("Programação Java", "programacaoJava", "Home")
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
    <footer>
        <p>System Hacker todos direitos reservados.</p>
    </footer>
</body>
</html>
